{
    "name": "stechstudio/laravel-percy",
    "description": "Integrates percy.io with Laravel Dusk for easy snapshot testing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "require": {
        "laravel/dusk": "1.*|2.*|3.*|4.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "STS\\Percy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\Percy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\Percy\\PercyServiceProvider"
            ]
        }
    }
}
